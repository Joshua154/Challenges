<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <mirrors>
        <mirror>
            <id>laudynetwork-master</id>
            <name>LaudyNetwork Master Repository</name>
            <url>https://repo.laudynetwork.com/repository/laudynetwork-master/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>laudynetwork-master</id>
            <username>${env.Nexus_REPO_USER}</username>
            <password>${env.Nexus_REPO_PASS}</password>
        </server>
    </servers>
</settings>
